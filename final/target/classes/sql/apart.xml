<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="apart">
	<insert id="insert" parameterType="apart">
		insert into apartment values(
			#{apart_id}, #{complex_id}, #{apart_dong}, #{apart_ho}, #{apart_floor}, 
			#{apart_area}, #{apart_price}, #{apart_room}, #{apart_toilet}, #{apart_interior} 
		)		
	</insert>
	
	<insert id="insert2" parameterType="java.util.List">
		<foreach collection="list" item="item" index="index"  open="INSERT ALL " separator=" " close="SELECT * FROM dual" > 
			into apartment values
	        (
				get_apart_seq, #{item.complex_id}, #{item.apart_dong}, #{item.apart_ho}, #{item.apart_floor}, 
				#{item.apart_area}, #{item.apart_price}, #{item.apart_room}, #{item.apart_toilet}, #{item.apart_interior} 
			)
	    </foreach>
	</insert>
	
</mapper>








